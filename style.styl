body
	font-family 'Open Sans'
	table
		width 90%
		text-align center
		box-shadow 0px 0px 5px rgba(0,0,0,.5)
		margin auto
		td
			padding .5em .25em
		th
			background-color #FBAE26
		tr:nth-of-type(odd)
			background-color #E8E8E8
		tr:nth-of-type(even)
			background-color #BAE8FF
	h2
		text-align center
		font-size 2em